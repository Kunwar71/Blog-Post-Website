<%- include("partials/header") -%>

<form action="/compose" method="post">
  <div class="form-group">
    <label style="margin: 0 15px 0 15px">Title</label>
    <input
      style="margin: 0 15px 0 15px"
      class="form-control"
      type="text"
      name="postTitle"
      autocomplete="off"
    />
    <label style="margin: 0 15px 0 15px">Content</label>
    <textarea
      style="margin: 0 15px 0 15px"
      class="form-control"
      cols="40"
      rows="10"
      name="postBody"
      autocomplete="off"
    ></textarea>
  </div>
  <button
    style="margin: 0 15px 0 15px"
    class="btn btn-primary"
    type="submit"
    name="button"
  >
    Publish
  </button>
</form>
<%- include("partials/footer") -%>
