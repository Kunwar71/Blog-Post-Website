<%- include("partials/header") -%>

<h1 style="margin: 0 15px 0 15px">Home</h1>
<p style="margin: 0 15px 0 15px"><%= homeStartingContent %></p>

<% posts.forEach(function(post) { %> <% if (post.name !== "Home") { %>
<h1 style="margin: 0 15px 0 15px"><%= post.name %></h1>
<p style="margin: 0 15px 0 15px">
  <%= post.content.substring(0, 100) %>...
  <a href="/post/<%= post._id %>">Read More!</a>
</p>

<% } %> <% }); %><%- include("partials/footer") -%>
