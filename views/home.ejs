<%- include("partials/header") -%>
<h1 style="margin: 0 15px 0 15px">Home</h1>
<p style="margin: 0 15px 0 15px"><%= homeStartingContent %></p>
<% posts.forEach(function(post){ %>
<h1 style="margin: 0 15px 0 15px"><%= post.postTitle %></h1>
<p style="margin: 0 15px 0 15px">
  <%= post.postBody.substring(0, 100) + " ..." %>
  <a href="/post/<%= post.postTitle %>">Read More!</a>
</p>
<% }) %> <%- include("partials/footer") -%>
